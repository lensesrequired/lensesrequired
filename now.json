{
  "name": "lensesrequired",
  "version": 2,
  "alias": ["ficdesigner.lensesrequired.com"],
  "builds": [
    { "src": "ficdesigner/next.config.js", "use": "@now/next" },
    { "src": "home/next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/ficdesigner/(_next|static)(.*)" },
    { "src": "/ficdesigner(.*)", "dest": "/ficdesigner$1", "continue": true },
    { "src": "(?!/?ficdesigner)(.*)", "dest": "/home$1", "continue": true }
  ],
  "build": {
    "env": {
      "BUILDING_FOR_NOW": "true"
    }
  }
}
