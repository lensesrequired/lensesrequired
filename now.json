{
  "name": "lensesrequired",
  "version": 2,
  "builds": [
    { "src": "home/next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/ficdesigner(.*)", "status": 301, "headers": { "Location": "https://ficdesigner.lensesrequired.com/$1" } },
    { "src": "(?!/?ficdesigner)(.*)", "dest": "/home$1", "continue": true }
  ]
}
